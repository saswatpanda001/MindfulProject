  "components": {
    "schemas": {
      "AffirmationEntry": {
        "required": [
          "category",
          "text",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "category": {
            "maxLength": 50,
            "minLength": 4,
            "type": "string"
          },
          "text": {
            "maxLength": 500,
            "minLength": 4,
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "MeditationSession": {
        "required": [
          "date",
          "durationMinutes",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "durationMinutes": {
            "maximum": 600,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "MoodEntry": {
        "required": [
          "date",
          "mood",
          "note",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "mood": {
            "maxLength": 20,
            "minLength": 3,
            "type": "string"
          },
          "note": {
            "maxLength": 500,
            "minLength": 4,
            "type": "string"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "User": {
        "required": [
          "email",
          "fullName",
          "password",
          "phone",
          "role"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "fullName": {
            "maxLength": 100,
            "minLength": 4,
            "type": "string"
          },
          "email": {
            "maxLength": 100,
            "minLength": 4,
            "type": "string",
            "format": "email"
          },
          "phone": {
            "minLength": 1,
            "pattern": "^\\d{10}$",
            "type": "string"
          },
          "location": {
            "maxLength": 100,
            "minLength": 4,
            "type": "string",
            "nullable": true
          },
          "password": {
            "minLength": 4,
            "type": "string"
          },
          "role": {
            "minLength": 1,
            "pattern": "^(User|Admin)$",
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "summary": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
